                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Inmueble encontrado"
                    },
                    "404": {
                        "description": "Inmueble encontrado"
                    }
                }
            }
        },
        "/api/storeNewPrice": {
            "post": {
                "tags": [
                    "Inmueble"
                ],
                "summary": " Store a new price.",
                "operationId": "19be59e3681a84876bcb0dbe1c441846",
                "parameters": [
                    {
                        "name": "id",
